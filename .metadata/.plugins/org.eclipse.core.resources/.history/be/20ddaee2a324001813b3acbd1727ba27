package com.troy.diplo.packet;

import com.troy.diplo.map.Map;

public class CreateMapData extends CreateData<Map> {
	
	private String name;
	private boolean enabled3D;

	public CreateMapData(String name, boolean enabled3D) {
		super(Map.class);
		this.name = name;
		this.enabled3D = enabled3D;
	}
	
	public String getName() {
		return name;
	}
	
	public boolean is3D() {
		return enabled3D;
	}

}
